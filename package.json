{
  "name": "youtube-downloader-electron",
  "version": "1.0.0",
  "description": "Free YouTube video downloader using Electron, React, and yt-dlp",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "react-bg": "cross-env BROWSER=none react-scripts start",
